{
  "name": "templecurraheen-graveyard",
  "version": "1.0.0",
  "description": "Historical documentation website for Templecurraheen Graveyard",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run css:watch\" \"eleventy --serve\"",
    "build": "npm run css:build && eleventy",
    "css:build": "tailwindcss -i ./src/css/styles.css -o ./_site/css/styles.css --minify",
    "css:watch": "tailwindcss -i ./src/css/styles.css -o ./_site/css/styles.css --watch",
    "deploy": "npm run build && gh-pages -d _site",
    "dev": "eleventy --serve"
  },
  "keywords": [
    "graveyard",
    "history",
    "documentation",
    "11ty"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "gh-pages": "^6.1.0",
    "tailwindcss": "^3.4.0"
  }
}
